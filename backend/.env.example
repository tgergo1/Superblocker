# Debug mode
DEBUG=false

# CORS origins (comma-separated)
CORS_ORIGINS=["http://localhost:5173", "http://localhost:3000"]

# Nominatim API settings
NOMINATIM_URL=https://nominatim.openstreetmap.org
NOMINATIM_USER_AGENT=Superblocker/1.0

# OSM settings
OSM_TIMEOUT=180
OSM_MEMORY_LIMIT=1073741824

# Cache settings
CACHE_ENABLED=true
CACHE_DIR=cache
CACHE_TTL_SECONDS=86400
CACHE_NETWORK_TTL_SECONDS=604800
CACHE_ANALYSIS_TTL_SECONDS=86400
CACHE_SEARCH_TTL_SECONDS=3600
